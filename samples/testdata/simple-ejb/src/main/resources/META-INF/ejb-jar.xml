<ejb-jar>
  <enterprise-beans>
    <session>
      <description>Sample Session EJB</description>
      <display-name>SampleEJB</display-name>
      <ejb-name>SampleEJB</ejb-name>
      <home>org.codehaus.cargo.sample.testdata.ejb.SampleHome</home>
      <remote>org.codehaus.cargo.sample.testdata.ejb.Sample</remote>
      <ejb-class>org.codehaus.cargo.sample.testdata.ejb.SampleBean</ejb-class>
      <session-type>Stateless</session-type>
